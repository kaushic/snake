void initSnake(){
 for (int i = 0; i < 20; i++){
 snake[i].width = 4;
 snake[i].height = 4;
 snake[i].row = 160/2 + (i * snake[i].height / 2);
 snake[i].col = 240/2 - (snake[i].width/2);
 snake[i].rdel = 4;
 snake[i].cdel = 4;
 snake[i].active = 0;
 snake[i].hitWall = 0;
 snake[i].hitSnake = 0;
 snake[i].prevAniState = 0;
 snake[i].curAniState = 0;
 }
 snake[0].active = 1;
 snakeLength++;
}

void drawSnake(){
 for (int i = 0; i < 20; i++){
  if(snake[i].active == 1){

   if (i == 0){
    shadowOAM[i].attr0 = snake[i].row | (0<<13) | (0<<14);
       shadowOAM[i].attr1 = snake[i].col | (0<<14);
       shadowOAM[i].attr2 = ((1)*32+(3));
   } else {
    shadowOAM[i].attr0 = snake[i].row | (0<<13) | (0<<14);
    shadowOAM[i].attr1 = snake[i].col | (0<<14);
    shadowOAM[i].attr2 = ((1)*32+(5));
   }
  }
 }
}

void updateSnake() {
 if (frameCounter % 10 == 0){
  for (int i = 20; i >= 0; i--){

   if (i > 0){
    snake[i].row = snake[i -1].row;
    snake[i].col = snake[i -1].col;
   }
   if (i == 0){
    switch(snake[0].curAniState) {
     case UP:
      snake[i].row -= snake[i].rdel;
      snake[0].prevAniState = UP;
      break;
     case DOWN:
      snake[i].row += snake[i].rdel;
      snake[0].prevAniState = DOWN;
      break;
     case LEFT:
      snake[i].col -= snake[i].cdel;
      snake[0].prevAniState = LEFT;
      break;
     case RIGHT:
      snake[i].col += snake[i].cdel;
      snake[0].prevAniState = RIGHT;
      break;
    }
   }
  }
 }

 if (snakeLength > 1){
  for (int j = 1; j < 20; j++){
   if (snake[j].active && collision(snake[0].col, snake[0].row, snake[0].width, snake[0].height, snake[j].col,
          snake[j].row,snake[j].width, snake[j].height)) {
    snake[0].hitSnake = 1;
   }
  }
 }


 if ((!(~(oldButtons)&((1<<5))) && (~buttons & ((1<<5)))) && snake[0].prevAniState != RIGHT) {
  snake[0].curAniState = LEFT;
 } else if ((!(~(oldButtons)&((1<<4))) && (~buttons & ((1<<4)))) && snake[0].prevAniState != LEFT) {
  snake[0].curAniState = RIGHT;
 } else if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && snake[0].prevAniState != DOWN) {
  snake[0].curAniState = UP;
 } else if ((!(~(oldButtons)&((1<<7))) && (~buttons & ((1<<7)))) && snake[0].prevAniState != UP) {
  snake[0].curAniState = DOWN;
 }

 if (snake[0].row < -1){
  snake[0].hitWall = 1;
 } else if (snake[0].row + snake[0].height >= 160){
  snake[0].hitWall = 1;
 } else if (snake[0].col < -1){
  snake[0].hitWall = 1;
 } else if (snake[0].col + snake[0].width >= 240){
  snake[0].hitWall = 1;
 }
}

void initApple(){
 for (int i = 0; i < 1; i++){
  apple[i].row = 0;
  apple[i].col = 0;
  apple[i].width = 16;
  apple[i].height = 16;
  apple[i].active = 0;
 }
}

void updateApple(){
 for (int i = 0; i < 1; i++){
  if (apple[i].active == 0 && appleLoc == -1){
   apple[i].row = (rand() % (160 - 30)) + 10;
   apple[i].col = (rand() % (240 - 30)) + 10;
   apple[i].active = 1;
   appleLoc = i;
   break;
  }
 }

 if (snake[0].active == 1){
  if (collision(apple[appleLoc].col, apple[appleLoc].row, apple[appleLoc].width, apple[appleLoc].height,
  snake[0].col, snake[0].row, snake[0].width, snake[0].height)){
    apple[appleLoc].active = 0;
    appleLoc = -1;
    applesLeft--;
    snakeLength++;
    for (int i = 0; i < 20; i++) {
     if (!snake[i].active) {
      snake[i].active = 1;
      break;
     }
    }
  }
 }
}

void drawApple(){
 for (int i = 0; i < 1; i++){
  if(apple[i].active == 1){
   shadowOAM[30 + i].attr0 = apple[i].row | (0<<13) | (0<<14);
   shadowOAM[30 + i].attr1 = apple[i].col | (1<<14);
   shadowOAM[30 + i].attr2 = ((0)*32+(0));
  }
 }
}
